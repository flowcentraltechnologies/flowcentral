<?xml version="1.0"?>
<!-- Workflow application -->
<application name="workflow"
	description="$m{workflow.short.description}" label="$m{workflow.label}"
	module="workflow">
	<!-- Widget Types -->
	<widgetTypes>
		<widgetType name="wfchanneldestinationlist"
			description="$m{workflow.widgettype.wfchanneldestinationlist}"
			dataType="string" inputType="string"
			editor="!ui-select list:wfchanneldestinationlist listParams:$l{direction entity} blankOption:$m{blank.none}"
			renderer="!ui-label" />
		<widgetType name="wfsteppolicylist"
			description="$m{workflow.widgettype.wfsteppolicylist}"
			dataType="string" inputType="string"
			editor="!ui-select list:wfsteppolicylist listParams:$l{type} blankOption:$m{blank.none}"
			renderer="!ui-label" />
		<widgetType name="wfsteprulelist"
			description="$m{workflow.widgettype.wfsteprulelist}"
			dataType="string" inputType="string"
			editor="!ui-select list:rulelist listParams:$l{policy} blankOption:$m{blank.none}"
			renderer="!ui-label" />
	</widgetTypes>
	<!-- Applets -->
	<applets>
		<applet name="myWorkItems"
			description="$m{workflowmyworkitems.application.workitems}"
			label="$m{workflowmyworkitems.application.workitems.label}" icon="copy"
			type="path_page" path="/workflow/myworkitems/openPage"
			menuAccess="true" allowSecondaryTenants="true">
		</applet>
		<applet name="exceptionWorkItems"
			description="$m{workflowexceptionworkitems.application.workitems}"
			label="$m{workflowexceptionworkitems.application.workitems.label}" icon="exclamation-circle"
			type="path_page" path="/workflow/exceptionworkitems/openPage"
			menuAccess="true" allowSecondaryTenants="true">
		</applet>
		<applet name="manageWorkflow"
			description="$m{workflow.workflow.manage}"
			label="$m{workflow.workflow.manage.label}" icon="project-diagram"
			type="manage_entitylist" entity="workflow" menuAccess="false">
			<property name="searchTable" value="workflowTable" />
			<property name="searchTable.new" value="false" />
			<property name="searchTable.report" value="true" />
			<property name="searchTable.basicSearchOnly" value="true" />
			<property name="maintainForm" value="workflowForm" />
		</applet>
		<applet name="manageInteractiveSteps"
			description="$m{workflow.step.interactive.manage}"
			label="$m{workflow.step.interactive.manage.label}"
			type="manage_entitylist" entity="wfStep" menuAccess="false">
			<property name="searchTable" value="wfStepTable" />
			<property name="searchTable.new" value="false" />
			<property name="searchTable.report" value="false" />
			<property name="maintainForm" value="wfStepForm" />
			<property name="baseRestriction" value="interactiveSteps" />
			<filter name="interactiveSteps"
				description="$m{workflow.steps.filter.interactive}">
				<restriction type="amongst" field="type"
					paramA="USR|ERR|SET|MRT|PRT|BRT|REC|ENR|PRC|NTF" />
			</filter>
		</applet>
		<applet name="manageWfStepRoles"
			description="$m{workflow.steprole.manage}"
			label="$m{workflow.steprole.manage.label}"
			type="manage_entitylist_assign" entity="wfStepRole"
			menuAccess="false">
			<property name="searchTable" value="wfStepRoleTable" />
			<property name="searchTable.edit" value="true" />
			<property name="searchTable.report" value="false" />
			<property name="assignmentPage" value="wfStepRoleAssignment" />
		</applet>
		<applet name="manageWorkflowChannel"
			description="$m{workflow.workflowchannel.manage}"
			label="$m{workflow.workflowchannel.manage.label}"
			icon="project-diagram" type="manage_entitylist"
			entity="workflowChannel" menuAccess="false">
			<property name="searchTable" value="workflowChannelTable" />
			<property name="searchTable.new" value="true" />
			<property name="searchTable.report" value="true" />
			<property name="searchTable.quickFilter" value="true" />
			<property name="createForm" value="workflowChannelForm" />
			<property name="createForm.save" value="true" />
			<property name="createForm.save.next" value="true" />
			<property name="createForm.save.close" value="true" />
			<property name="maintainForm" value="workflowChannelForm" />
			<property name="maintainForm.update" value="true" />
			<property name="maintainForm.delete" value="true" />
			<property name="maintainForm.delete.condition"
				value="delatableWorkflowChannel" />
			<filter name="delatableWorkflowChannel"
				description="$m{workflow.workflowchannel.filter.deletableworkflowchannel}">
				<restriction type="equals" field="configType" paramA="C" />
			</filter>
		</applet>
		<applet name="workflowItem"
			description="$m{workflow.wfitem.manage}"
			label="$m{workflow.wfitem.manage.label}" icon="list"
			type="manage_entitylist" entity="wfItem" menuAccess="false">
			<property name="searchTable" value="wfItemTable" />
			<property name="searchTable.new" value="false" />
			<property name="searchTable.report" value="true" />
			<property name="maintainForm" value="wfItemForm" />
		</applet>
	</applets>
	<!-- References -->
	<references>
		<reference name="workflowRef"
			description="$m{workflow.workflow.reference}" entity="workflow"
			searchField="description" />
		<reference name="workflowFilterRef"
			description="$m{workflow.workflowfilter.reference}"
			entity="workflowFilter" searchField="description" />
		<reference name="workflowSetValuesRef"
			description="$m{workflow.workflowsetvalues.reference}"
			entity="workflowSetValues" searchField="description" />
		<reference name="wfStepRef"
			description="$m{workflow.step.reference}" entity="wfStep"
			searchField="description" />
		<reference name="wfStepSetValuesRef"
			description="$m{workflow.stepsetvalues.reference}"
			entity="wfStepSetValues" searchField="description" />
		<reference name="wfStepRoutingRef"
			description="$m{workflow.steprouting.reference}"
			entity="wfStepRouting" searchField="description" />
		<reference name="wfStepUserActionRef"
			description="$m{workflow.stepuseraction.reference}"
			entity="wfStepUserAction" searchField="description" />
		<reference name="wfStepAlertRef"
			description="$m{workflow.stepalert.reference}" entity="wfStepAlert"
			searchField="description" />
		<reference name="wfStepRoleRef"
			description="$m{workflow.steprole.reference}" entity="wfStepRole"
			searchField="description" />
		<reference name="wfItemHistRef"
			description="$m{workflow.wfitemhist.reference}" entity="wfItemHist" />
		<reference name="wfItemEventRef"
			description="$m{workflow.wfitemevent.reference}" entity="wfItemEvent" />
		<reference name="wfItemRef"
			description="$m{workflow.wfitem.reference}" entity="wfItem" />
		<reference name="workflowChannelRef"
			description="$m{workflow.workflowchannel.reference}"
			entity="workflowChannel" searchField="description" />
	</references>
	<!-- Entities -->
	<entities>
		<entity name="workflow" description="$m{workflow.workflow}"
			label="$m{workflow.workflow}"
			type="com.flowcentraltech.flowcentral.workflow.entities.Workflow"
			reportable="true" auditable="true">
			<field name="label" label="$m{workflow.workflow.label}"
				type="string" maxLen="64" />
			<field name="entity" label="$m{workflow.workflow.entity}"
				type="string" maxLen="128" />
			<field name="loadingTable" label="$m{workflow.workflow.loadingtable}"
				type="string" maxLen="128" nullable = "true"/>
			<field name="appletVersionNo"
				label="$m{workflow.workflow.appletversionno}" type="long" />
			<field name="descFormat"
				label="$m{workflow.workflow.itemdescformat}" type="string"
				nullable="true" />
			<field name="filterList" label="$m{workflow.workflow.filters}"
				type="child_list" references="workflowFilterRef" />
			<field name="setValuesList"
				label="$m{workflow.workflow.setvalues}" type="child_list"
				references="workflowSetValuesRef" />
			<field name="stepList" label="$m{workflow.workflow.steps}"
				type="child_list" references="wfStepRef" />
			<uniqueConstraint name="nameUc"
				description="$m{workflow.workflow.uc.name}"
				fieldList="applicationId,name">
				<condition field="devVersionType" value="CRN" />
			</uniqueConstraint>
			<uniqueConstraint name="descUc"
				description="$m{workflow.workflow.uc.description}"
				fieldList="applicationId,description">
				<condition field="devVersionType" value="CRN" />
			</uniqueConstraint>
		</entity>
		<entity name="workflowFilter"
			description="$m{workflow.workflowfilter}"
			label="$m{workflow.workflowfilter.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WorkflowFilter">
			<field name="workflowId"
				label="$m{workflow.workflowfilter.workflowid}" type="ref"
				references="workflowRef" inputWidget="application.entitylist" />
			<field name="filterGenerator"
				label="$m{workflow.workflowfilter.filtergenerator}" type="string"
				nullable="true" />
			<field name="filterGeneratorRule"
				label="$m{workflow.workflowfilter.filtergeneratorrule}"
				type="string" nullable="true" />
			<field name="filter" label="$m{workflow.workflowfilter.filter}"
				type="child" category="workflow"
				references="application.appFilterRef" />
			<uniqueConstraint name="nameUc"
				description="$m{workflow.workflowfilter.uc.name}"
				fieldList="workflowId,name" />
			<uniqueConstraint name="descUc"
				description="$m{workflow.workflowfilter.uc.description}"
				fieldList="workflowId,description" />
		</entity>
		<entity name="workflowSetValues"
			description="$m{workflow.workflowsetvalues}"
			label="$m{workflow.workflowsetvalues.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WorkflowSetValues">
			<field name="workflowId"
				label="$m{workflow.workflowsetvalues.workflowid}" type="ref"
				references="workflowRef" inputWidget="application.entitylist" />
			<field name="type" label="$m{workflow.workflowsetvalues.type}"
				type="enum_ref" references="wfsetvaluestypelist"
				inputWidget="application.enumlist" />
			<field name="onCondition"
				label="$m{workflow.workflowsetvalues.oncondition}" type="child"
				category="workflow" references="application.appFilterRef" />
			<field name="valueGenerator"
				label="$m{workflow.workflowsetvalues.valuegenerator}" type="string"
				nullable="true" />
			<field name="setValues"
				label="$m{workflow.workflowsetvalues.setvalue}" type="child"
				category="workflow" references="application.appSetValuesRef" />
			<field name="typeDesc"
				label="$m{workflow.workflowsetvalues.typedesc}" type="list_only"
				key="type" property="description" />
			<uniqueConstraint name="nameUc"
				description="$m{workflow.workflowsetvalues.uc.name}"
				fieldList="workflowId,name" />
			<uniqueConstraint name="descUc"
				description="$m{workflow.workflowsetvalues.uc.description}"
				fieldList="workflowId,description" />
		</entity>
		<entity name="wfStep" description="$m{workflow.step}"
			label="$m{workflow.step}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStep"
			reportable="false" auditable="false">
			<field name="workflowId" label="$m{workflow.step.workflowid}"
				type="ref" references="workflowRef"
				inputLabel="$m{workflow.step.workflow}"
				inputWidget="application.entitysearch" />
			<field name="type" label="$m{workflow.step.type}"
				type="enum_ref" references="wfsteptypelist"
				inputWidget="application.enumlist" />
			<field name="valueGenerator"
				label="$m{workflow.step.valuegenerator}" type="string"
				nullable="true" />
			<field name="priority" label="$m{workflow.step.priority}"
				type="enum_ref" references="wfstepprioritylist"
				inputWidget="application.enumlist" />
			<field name="recordActionType"
				label="$m{workflow.step.recordactiontype}" type="enum_ref"
				references="recordactiontypelist" inputWidget="application.enumlist" />
			<field name="label" label="$m{workflow.step.label}"
				type="string" maxLen="64" />
			<field name="nextStepName"
				label="$m{workflow.step.nextstepname}" type="string" nullable="true" />
			<field name="binaryConditionName"
				label="$m{workflow.step.binarycondition}" type="string" nullable="true" />
			<field name="readOnlyConditionName"
				label="$m{workflow.step.readonlycondition}" type="string" nullable="true" />
			<field name="autoLoadConditionName"
				label="$m{workflow.step.autoloadingcondition}" type="string" nullable="true" />
			<field name="attachmentProviderName"
				label="$m{workflow.step.attachmentprovidername}" type="string" nullable="true" />
			<field name="policy" label="$m{workflow.step.policy}"
				type="string" maxLen="64" nullable="true"
				inputWidget="wfsteppolicylist" />
			<field name="rule" label="$m{workflow.step.rule}" type="string"
				maxLen="64" nullable="true" inputWidget="wfsteprulelist" />
			<field name="criticalMinutes"
				label="$m{workflow.step.criticalminutes}" type="integer" />
			<field name="expiryMinutes"
				label="$m{workflow.step.expiryminutes}" type="integer" />
			<field name="audit" label="$m{workflow.step.audit}"
				type="boolean" />
			<field name="branchOnly" label="$m{workflow.step.branchonly}"
				type="boolean" />
			<field name="includeForwarder"
				label="$m{workflow.step.includeforwarder}" type="boolean" />
			<field name="forwarderPreffered"
				label="$m{workflow.step.forwarderpreffered}" type="boolean" />
			<field name="emails" label="$m{workflow.step.emails}"
				type="string" />
			<field name="comments" label="$m{workflow.step.comments}"
				type="string" />
			<field name="appletSetValuesName" label="$m{workflow.step.appletsetvaluesname}"
				type="string" />
			<field name="typeDesc" label="$m{workflow.step.typedesc}"
				type="list_only" key="type" property="description" />
			<field name="priorityDesc"
				label="$m{workflow.step.prioritydesc}" type="list_only"
				key="priority" property="description" />
			<field name="recordActionTypeDesc"
				label="$m{workflow.step.recordactiontypedesc}" type="list_only"
				key="recordActionType" property="description" />
			<field name="workflowName"
				label="$m{workflow.step.workflowname}" type="list_only"
				key="workflowId" property="name" />
			<field name="workflowDesc"
				label="$m{workflow.step.workflowdesc}" type="list_only"
				key="workflowId" property="description" />
			<field name="applicationName"
				label="$m{workflow.step.applicationname}" type="list_only"
				key="workflowId" property="applicationName" />
			<field name="applicationDesc"
				label="$m{workflow.step.applicationdesc}" type="list_only"
				key="workflowId" property="applicationDesc" />
			<field name="routingList" label="$m{workflow.step.routings}"
				type="child_list" references="wfStepRoutingRef" />
			<field name="userActionList"
				label="$m{workflow.step.useractions}" type="child_list"
				references="wfStepUserActionRef" />
			<field name="alertList" label="$m{workflow.step.alerts}"
				type="child_list" references="wfStepAlertRef" />
			<field name="roleList" label="$m{workflow.step.roles}"
				type="child_list" references="wfStepRoleRef" />
		</entity>
		<entity name="wfStepSetValues"
			description="$m{workflow.stepsetvalues}"
			label="$m{workflow.stepsetvalues.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepSetValues"
			reportable="false" auditable="false">
			<field name="wfStepId"
				label="$m{workflow.stepsetvalues.stepid}" type="ref"
				references="wfStepRef" inputLabel="$m{workflow.stepsetvalues.step}"
				inputWidget="application.entitylist" />
			<field name="wfStepDesc"
				label="$m{workflow.stepsetvalues.stepdesc}" type="list_only"
				key="wfStepId" property="description" />
			<field name="setValues"
				label="$m{workflow.stepsetvalues.values}" type="child"
				category="wfstep" references="application.appSetValuesRef" />
		</entity>
		<entity name="wfStepRouting"
			description="$m{workflow.steprouting}"
			label="$m{workflow.steprouting.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepRouting"
			reportable="false" auditable="false">
			<field name="wfStepId" label="$m{workflow.steprouting.stepid}"
				type="ref" references="wfStepRef"
				inputLabel="$m{workflow.steprouting.step}"
				inputWidget="application.entitylist" />
			<field name="nextStepName"
				label="$m{workflow.steprouting.nextstepname}" type="string"
				maxLen="64" />
			<field name="conditionName"
				label="$m{workflow.steprouting.filter}" type="string" maxLen="64" />
			<field name="wfStepDesc"
				label="$m{workflow.steprouting.stepdesc}" type="list_only"
				key="wfStepId" property="description" />
		</entity>
		<entity name="wfStepUserAction"
			description="$m{workflow.stepuseraction}"
			label="$m{workflow.stepuseraction.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepUserAction"
			reportable="false" auditable="false">
			<field name="wfStepId"
				label="$m{workflow.stepuseraction.stepid}" type="ref"
				references="wfStepRef" inputLabel="$m{workflow.stepuseraction.step}"
				inputWidget="application.entitylist" />
			<field name="commentRequirement"
				label="$m{workflow.stepuseraction.commentrequirement}"
				type="enum_ref" references="requirementtypelist"
				inputWidget="application.enumlist" />
			<field name="highlightType"
				label="$m{workflow.stepuseraction.highlighttype}" type="enum_ref"
				references="highlighttypelist" inputWidget="application.enumlist"
				nullable="true" />
			<field name="label" label="$m{workflow.stepuseraction.label}"
				type="string" maxLen="96" />
			<field name="nextStepName"
				label="$m{workflow.stepuseraction.nextstepname}" type="string"
				maxLen="64" />
			<field name="setValuesName"
				label="$m{workflow.stepuseraction.setvaluesname}" type="string"
				maxLen="64" nullable="true" />
			<field name="appletSetValuesName"
				label="$m{workflow.stepuseraction.appletsetvaluesname}"
				type="string" maxLen="64" nullable="true" />
			<field name="showOnCondition"
				label="$m{workflow.stepuseraction.showoncondition}"
				type="string" maxLen="64" nullable="true" />
			<field name="orderIndex"
				label="$m{workflow.stepuseraction.orderindex}" type="integer" />
			<field name="formReview"
				label="$m{workflow.stepuseraction.formreview}" type="boolean" />
			<field name="validatePage"
				label="$m{workflow.stepuseraction.validatepage}" type="boolean" />
			<field name="wfStepDesc"
				label="$m{workflow.stepuseraction.stepdesc}" type="list_only"
				key="wfStepId" property="description" />
			<field name="commentRequirementDesc"
				label="$m{workflow.stepuseraction.commentrequirementdesc}"
				type="list_only" key="commentRequirement" property="description" />
			<field name="highlightTypeDesc"
				label="$m{workflow.stepuseraction.highlighttypedesc}"
				type="list_only" key="highlightType" property="description" />
		</entity>
		<entity name="wfStepAlert" description="$m{workflow.stepalert}"
			label="$m{workflow.stepalert.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepAlert"
			reportable="false" auditable="false">
			<field name="wfStepId" label="$m{workflow.stepalert.stepid}"
				type="ref" references="wfStepRef"
				inputLabel="$m{workflow.stepalert.step}"
				inputWidget="application.entitylist" />
			<field name="type" label="$m{workflow.stepalert.type}"
				type="enum_ref" references="wfalerttypelist"
				inputWidget="application.enumlist" />
			<field name="notificationType"
				label="$m{workflow.stepalert.notificationtype}" type="enum_ref"
				references="notificationtypelist" inputWidget="application.enumlist" />
			<field name="recipientPolicy"
				label="$m{workflow.stepalert.recipientpolicy}" type="string"
				maxLen="64" nullable="true" />
			<field name="recipientNameRule"
				label="$m{workflow.stepalert.recipientnamerule}" type="string"
				maxLen="64" nullable="true" />
			<field name="recipientContactRule"
				label="$m{workflow.stepalert.recipientcontactrule}" type="string"
				maxLen="64" nullable="true" />
			<field name="generator"
				label="$m{workflow.stepalert.generator}" type="string" maxLen="128" />
			<field name="fireOnPrevStepName"
				label="$m{workflow.stepalert.fireonprevstepname}" type="string"
				nullable="true" maxLen="64" />
			<field name="fireOnConditionName"
				label="$m{workflow.stepalert.fireonfilter}" type="string"
				nullable="true" maxLen="64" />
			<field name="wfStepDesc"
				label="$m{workflow.stepalert.stepdesc}" type="list_only"
				key="wfStepId" property="description" />
			<field name="notificationTypeDesc"
				label="$m{workflow.stepalert.notificationtypedesc}" type="list_only"
				key="notificationType" property="description" />
			<field name="typeDesc" label="$m{workflow.stepalert.typedesc}"
				type="list_only" key="type" property="description" />
		</entity>
		<entity name="wfStepRole" description="$m{workflow.steprole}"
			label="$m{workflow.steprole.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfStepRole"
			reportable="false" auditable="false">
			<field name="wfStepId" label="$m{workflow.steprole.stepid}"
				type="ref" references="wfStepRef"
				inputLabel="$m{workflow.steprole.step}"
				inputWidget="application.entitylist" />
			<field name="roleId" label="$m{workflow.steprole.roleid}"
				type="ref" references="organization.roleRef"
				inputLabel="$m{workflow.steprole.role}"
				inputWidget="application.entitysearch" />
			<field name="wfStepName" label="$m{workflow.steprole.stepname}"
				type="list_only" key="wfStepId" property="name" />
			<field name="wfStepDesc" label="$m{workflow.steprole.stepdesc}"
				type="list_only" key="wfStepId" property="description" />
			<field name="workflowName"
				label="$m{workflow.steprole.workflowname}" type="list_only"
				key="wfStepId" property="workflowName" />
			<field name="workflowDesc"
				label="$m{workflow.steprole.workflowdesc}" type="list_only"
				key="wfStepId" property="workflowDesc" />
			<field name="applicationName"
				label="$m{workflow.steprole.applicationname}" type="list_only"
				key="wfStepId" property="applicationName" />
			<field name="applicationDesc"
				label="$m{workflow.steprole.applicationdesc}" type="list_only"
				key="wfStepId" property="applicationDesc" />
			<field name="roleCode" label="$m{workflow.steprole.rolecode}"
				type="list_only" key="roleId" property="code" />
			<field name="roleDesc" label="$m{workflow.steprole.roledesc}"
				type="list_only" key="roleId" property="description" />
		</entity>
		<entity name="wfItemHist" description="$m{workflow.wfitemhist}"
			label="$m{workflow.wfitemhist.label}"
			type="com.flowcentraltech.flowcentral.workflow.entities.WfItemHist">
			<field name="applicationName"
				label="$m{workflow.wfitemhist.applicationname}" type="string"
				maxLen="64" />
			<field name="workflowName"
				label="$m{workflow.wfitemhist.workflowname}" type="string"
				maxLen="128" />
			<field name="entity" label="$m{workflow.wfitemhist.entity}"
				type="string" maxLen="128" />
			<field name="originWorkRecId"
				label="$m{workflow.wfitemhist.originworkrecid}" type="long"
				nullable="true" />
			<field name="branchCode"
				label="$m{workflow.wfitemhist.branchcode}" type="string" maxLen="32"
				nullable="true" />
