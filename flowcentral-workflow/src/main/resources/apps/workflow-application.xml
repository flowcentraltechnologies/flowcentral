	<!-- Forms -->
	<forms>
		<form name="workflowForm"
			description="$m{workflow.workflow.form}" entity="workflow">
			<tab name="basicDetails"
				label="$m{workflow.workflow.form.basicdetails}"
				contentType="miniform" editable="false">
				<section name="workflowDetails" columns="type_2">
					<field name="applicationId"
						inputWidget="application.entitylist" column="0"
						label="$m{workflow.workflow.application}" required="true" />
					<field name="name" inputWidget="application.name" column="0"
						required="true" />
					<field name="description" inputWidget="application.text"
						column="0" required="true" />
					<field name="label" inputWidget="application.text" column="0"
						required="true" />
					<field name="entity"
						inputWidget="application.appworkappentitylist" column="1"
						required="true" />
				</section>
			</tab>
			<tab name="interactiveSteps"
				label="$m{workflow.workflow.form.interactivesteps}"
				contentType="child_list" applet="manageInteractiveSteps"
				reference="stepList" />
		</form>
		<form name="wfStepForm" description="$m{workflow.step.form}"
			entity="wfStep">
