						required="true" />
					<field name="description" inputWidget="application.text"
						column="0" required="true" />
					<field name="label" inputWidget="application.text" column="0"
						required="true" />
					<field name="type" inputWidget="application.enumlist"
						column="1" required="true" />
					<field name="recordActionType"
						inputWidget="application.enumlist" column="1" required="true"
						visible="false" />
					<field name="policy" column="1" required="true"
						visible="false" />
					<field name="rule" column="1" visible="false" />
					<field name="priority" inputWidget="application.enumlist"
						column="1" required="true" />
					<field name="criticalMinutes"
						inputWidget="application.integer" column="1" />
					<field name="expiryMinutes" inputWidget="application.integer"
						column="1" />
					<field name="audit" inputWidget="application.checkbox"
						column="1" />
					<field name="branchOnly" inputWidget="application.checkbox"
						column="1" />
					<field name="includeForwarder"
						inputWidget="application.checkbox" column="1" />
				</section>
			</tab>
			<tab name="assignedRoles"
				label="$m{workflow.step.form.assignedroles}"
				contentType="child_list" applet="manageWfStepRoles"
				reference="roleList" />
			<formStatePolicy type="on_switch" name="policyPolicy"
				description="$m{workflow.step.form.fieldstatepolicy.policy}">
				<onCondition>
					<restriction type="amongst" field="type" paramA="N|P" />
				</onCondition>
				<setStates>
					<setState type="field" target="policy,rule" visible="true" />
				</setStates>
			</formStatePolicy>
			<formStatePolicy type="on_switch"
				name="recordActionPolicy"
				description="$m{workflow.step.form.fieldstatepolicy.recordactiontype}">
				<onCondition>
					<restriction type="equals" field="type" paramA="R" />
				</onCondition>
				<setStates>
					<setState type="field" target="recordActionType"
						visible="true" />
				</setStates>
			</formStatePolicy>
		</form>
		<form name="wfItemForm" description="$m{workflow.wfitem.form}"
			entity="wfItem">
			<tab name="basicDetails"
				label="$m{workflow.wfitem.form.basicdetails}" contentType="miniform"
				editable="false">
				<section name="wfItemDetails" columns="type_2">
					<field name="applicationName" inputWidget="application.text"
						column="0" required="true" />
					<field name="workflowName" inputWidget="application.text"
						column="0" required="true" />
					<field name="wfStepName" inputWidget="application.text"
						column="0" required="true" />
					<field name="wfItemDesc" inputWidget="application.text"
						column="0" required="true" />
					<field name="forwardedBy" inputWidget="application.text"
						column="0" required="true" />
					<field name="heldBy" inputWidget="application.text"
						column="0" />
					<field name="priorityDesc" inputWidget="application.text"
						column="1" required="true" />
					<field name="stepDt" inputWidget="application.datetime"
						column="1" required="true" />
					<field name="criticalDt" inputWidget="application.datetime"
						column="1" />
					<field name="expectedDt" inputWidget="application.datetime"
						column="1" />
					<field name="criticalAlertSent"
						inputWidget="application.checkbox" column="1" />
					<field name="expirationAlertSent"
						inputWidget="application.checkbox" column="1" />
				</section>
				<section name="errorDetails" columns="type_1">
					<field name="errorCode" inputWidget="application.text"
						column="0" />
					<field name="errorMsg" inputWidget="application.textarea"
						column="0" />
				</section>
			</tab>
		</form>
		<form name="workflowChannelForm"
			description="$m{workflow.workflowchannel.form}"
			entity="workflowChannel">
			<tab name="basicDetails"
				label="$m{workflow.workflowchannel.form.basicdetails}"
				contentType="miniform">
				<section name="workflowChannelDetails" columns="type_2">
					<field name="applicationId"
						inputWidget="application.entitylist" column="0"
						label="$m{workflow.workflowchannel.application}"
						switchOnChange="true" required="true" disabled="false" />
					<field name="name" inputWidget="application.name" column="0"
						required="true" disabled="true" />
					<field name="description" inputWidget="application.text"
						column="0" required="true" disabled="true" />
					<field name="label" inputWidget="application.text" column="0"
						required="true" />
					<field name="direction" inputWidget="application.enumlist"
						column="1" required="true" disabled="true" />
					<field name="entity"
						inputWidget="application.appworkappentitylist" column="1"
						required="true" disabled="true" switchOnChange="true" />
					<field name="destination"
						inputWidget="wfchanneldestinationlist" column="1" required="true" />
					<field name="rule" inputWidget="application.text" column="1" />
					<field name="status" inputWidget="application.enumlist"
						column="1" required="true" />
				</section>
			</tab>
			<tab name="changeLog" label="$m{application.form.audit}"
				contentType="miniform_changelog" />
			<formStatePolicy type="on_switch" name="editPolicy"
				description="$m{workflow.workflowchannel.form.fieldstatepolicy.editpolicy}">
				<onCondition>
					<restriction type="equals" field="configType"
						paramA="C" />
				</onCondition>
				<setStates>
					<setState type="field"
						target="applicationId,name,description,direction,entity"
						disabled="false" />
				</setStates>
			</formStatePolicy>
		</form>
	</forms>
