<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
   	targetNamespace="http://flowcentraltech.com/schema-common"
   	xmlns="http://flowcentraltech.com/schema-common"
	attributeFormDefault="unqualified" elementFormDefault="qualified">
  <xs:include schemaLocation="flowcentral-types.xsd"/>
  <!-- Application -->
  <xs:element name="application">
    <xs:complexType>
      <xs:all minOccurs="0" maxOccurs="1">
        <!-- Reports -->
        <xs:element name="reports" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="report" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="xs:string" name="configFile" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Help sheets -->
        <xs:element name="helpSheets" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="helpSheet" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="xs:string" name="configFile" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Notification Templates -->
        <xs:element name="notifTemplates" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="notifTemplate" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="xs:string" name="configFile" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Workflows -->
        <xs:element name="workflows" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="workflow" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="xs:string" name="configFile" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Workflow wizards -->
        <xs:element name="workflow-wizards" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="workflow-wizard" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="xs:string" name="configFile" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Applets -->
        <xs:element name="applets" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="applet" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType mixed="true">
                  <xs:all>
                    <xs:element name="properties" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="property" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="value" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="routeToAppletItems" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="routeToAppletItem" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="applet" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="alerts" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="alert" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="optional"/>
                              <xs:attribute type="xs:string" name="sender" use="optional"/>
                              <xs:attribute type="xs:string" name="template" use="optional"/>
                              <xs:attribute type="xs:string" name="recipientPolicy" use="optional"/>
                              <xs:attribute type="xs:string" name="recipientNameRule" use="optional"/>
                              <xs:attribute type="xs:string" name="recipientContactRule" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="filters" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="filter" type="filter" maxOccurs="unbounded" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="valuesset" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="values" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="setValues" type="setvalues" maxOccurs="1" minOccurs="0"/>
                              </xs:sequence>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="optional"/>
                              <xs:attribute type="xs:string" name="valueGenerator" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                  <xs:attribute type="applettype" name="type" use="required"/>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                  <xs:attribute type="xs:string" name="description" use="required"/>
                  <xs:attribute type="xs:string" name="label" use="required"/>
                  <xs:attribute type="xs:string" name="entity" use="optional"/>
                  <xs:attribute type="xs:string" name="icon" use="optional"/>
                  <xs:attribute type="xs:string" name="path" use="optional"/>
                  <xs:attribute type="xs:string" name="titleFormat" use="optional"/>
                  <xs:attribute type="xs:integer" name="displayIndex" use="optional"/>
                  <xs:attribute type="xs:boolean" name="menuAccess" use="optional"/>
                  <xs:attribute type="xs:boolean" name="supportOpenInNewWindow" use="optional"/>
                  <xs:attribute type="xs:boolean" name="allowSecondaryTenants" use="optional"/>
                  <xs:attribute type="xs:string" name="routeToApplet" use="optional"/>
                  <xs:attribute type="xs:string" name="baseField" use="optional"/>
                  <xs:attribute type="xs:string" name="assignField" use="optional"/>
                  <xs:attribute type="xs:string" name="assignDescField" use="optional"/>
                  <xs:attribute type="xs:string" name="pseudoDeleteField" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Widget Types -->
        <xs:element name="widgetTypes" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="widgetType" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="datatype" name="dataType" use="required"/>
                  <xs:attribute type="inputtype" name="inputType" use="optional"/>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                  <xs:attribute type="xs:string" name="description" use="required"/>
                  <xs:attribute type="xs:string" name="editor" use="required"/>
                  <xs:attribute type="xs:string" name="renderer" use="optional"/>
                  <xs:attribute type="xs:boolean" name="stretch" use="optional"/>
                  <xs:attribute type="xs:boolean" name="listOption" use="optional"/>
                  <xs:attribute type="xs:boolean" name="enumOption" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- References -->
        <xs:element name="references" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="reference" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="filter" type="filter" maxOccurs="1" minOccurs="0"/>
                  </xs:sequence>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                  <xs:attribute type="xs:string" name="description" use="required"/>
                  <xs:attribute type="xs:string" name="entity" use="required"/>
                  <xs:attribute type="xs:string" name="searchField" use="optional"/>
                  <xs:attribute type="xs:string" name="orderField" use="optional"/>
                  <xs:attribute type="xs:string" name="searchTable" use="optional"/>
                  <xs:attribute type="xs:string" name="selectHandler" use="optional"/>
                  <xs:attribute type="xs:string" name="listFormat" use="optional"/>
                  <xs:attribute type="xs:string" name="filterGenerator" use="optional"/>
                  <xs:attribute type="xs:string" name="filterGeneratorRule" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Entities -->
        <xs:element name="entities" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="entity" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType mixed="true">
                  <xs:all>
                    <xs:element name="fields" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="field" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="fielddatatype" name="type" use="required"/>
                                  <xs:attribute name="case" use="optional">
                                    <xs:simpleType>
                                      <xs:restriction base="xs:string">
                                        <xs:enumeration value="LOWER"/>
                                        <xs:enumeration value="UPPER"/>
                                        <xs:enumeration value="CAMEL"/>
                                        <xs:enumeration value="CAPITAL"/>
                                        <xs:enumeration value="PATH"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:attribute>
                                  <xs:attribute type="xs:string" name="name" use="required"/>
                                  <xs:attribute type="xs:string" name="label" use="required"/>
                                  <xs:attribute type="xs:string" name="inputLabel" use="optional"/>
                                  <xs:attribute type="xs:string" name="inputWidget" use="optional"/>
                                  <xs:attribute type="xs:string" name="inputListKey" use="optional"/>
                                  <xs:attribute type="xs:string" name="suggestionType" use="optional"/>
                                  <xs:attribute type="xs:string" name="lingualWidget" use="optional"/>
                                  <xs:attribute type="xs:string" name="lingualListKey" use="optional"/>
                                  <xs:attribute type="xs:string" name="autoFormat" use="optional"/>
                                  <xs:attribute type="xs:string" name="defaultVal" use="optional"/>
                                  <xs:attribute type="xs:string" name="category" use="optional"/>
                                  <xs:attribute type="xs:integer" name="maxLen" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="trim" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="allowNegative" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="readOnly" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="nullable" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="auditable" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="reportable" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="descriptive" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="basicSearch" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="branchScoping" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="maintainLink" use="optional"/>
                                  <xs:attribute type="xs:string" name="column" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="maintainAction" use="optional"/>
                                  <xs:attribute type="xs:string" name="references" use="optional"/>
                                  <xs:attribute type="xs:integer" name="minLen" use="optional"/>
                                  <xs:attribute type="xs:integer" name="columns" use="optional"/>
                                  <xs:attribute type="xs:integer" name="rows" use="optional"/>
                                  <xs:attribute type="xs:integer" name="precision" use="optional"/>
                                  <xs:attribute type="xs:integer" name="scale" use="optional"/>
                                  <xs:attribute type="xs:string" name="key" use="optional"/>
                                  <xs:attribute type="xs:string" name="property" use="optional"/>
                                  <xs:attribute type="xs:string" name="mapped" use="optional"/>
                                  <xs:attribute type="xs:string" name="jsonName" use="optional"/>
                                  <xs:attribute type="xs:string" name="jsonFormatter" use="optional"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="expressions" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="expression" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="expression" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="searchInputs" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="searchInput" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="input" maxOccurs="unbounded" minOccurs="0">
                                  <xs:complexType>
                                    <xs:attribute type="searchcondtype" name="type" use="required"/>
                                    <xs:attribute type="xs:string" name="field" use="required"/>
                                    <xs:attribute type="xs:string" name="widget" use="required"/>
                                    <xs:attribute type="xs:string" name="label" use="required"/>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="restrictionResolver" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="seriesset" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="series" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute name="type" use="required">
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:enumeration value="COUNT"/>
                                    <xs:enumeration value="SUM"/>
                                    <xs:enumeration value="AVERAGE"/>
                                    <xs:enumeration value="MINIMUM"/>
                                    <xs:enumeration value="MAXIMUM"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="required"/>
                              <xs:attribute type="xs:string" name="fieldName" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="categories" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="category" type="filter" maxOccurs="unbounded" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="uniqueConstraints" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="uniqueConstraint" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="condition" maxOccurs="unbounded" minOccurs="0">
                                  <xs:complexType>
                                    <xs:attribute type="xs:string" name="field" use="required"/>
                                    <xs:attribute type="xs:string" name="value" use="required"/>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="fieldList" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="attachments" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="attachment" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute name="type" use="required">
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:enumeration value="AUDIO"/>
                                    <xs:enumeration value="CSV"/>
                                    <xs:enumeration value="EXCEL"/>
                                    <xs:enumeration value="IMAGE"/>
                                    <xs:enumeration value="IMAGE_PNG"/>
                                    <xs:enumeration value="IMAGE_JPG"/>
                                    <xs:enumeration value="IMAGE_GIF"/>
                                    <xs:enumeration value="IMAGE_BMP"/>
                                    <xs:enumeration value="PDF"/>
                                    <xs:enumeration value="XML"/>
                                    <xs:enumeration value="TEXT"/>
                                    <xs:enumeration value="VIDEO"/>
                                    <xs:enumeration value="WILDCARD"/>
                                    <xs:enumeration value="WORD"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="indexes" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="index" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="fieldList" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="uploads" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="upload" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element type="fieldsequence" name="fieldSequence" maxOccurs="1" minOccurs="0"/>
                              </xs:sequence>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="optional"/>
                              <xs:attribute name="onConstraint" use="required">
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:enumeration value="UPDATE"/>
                                    <xs:enumeration value="SKIP"/>
                                    <xs:enumeration value="FAIL"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                  <xs:attribute type="entitybasetype" name="baseType" use="optional"/>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                  <xs:attribute type="xs:string" name="description" use="required"/>
                  <xs:attribute type="xs:string" name="label" use="required"/>
                  <xs:attribute type="xs:string" name="type" use="required"/>
                  <xs:attribute type="xs:string" name="emailProducerConsumer" use="optional"/>
                  <xs:attribute type="xs:string" name="delegate" use="optional"/>
                  <xs:attribute type="xs:string" name="table" use="optional"/>
                  <xs:attribute type="xs:string" name="dataSourceName" use="optional"/>
                  <xs:attribute type="xs:string" name="dateFormatter" use="optional"/>
                  <xs:attribute type="xs:string" name="dateTimeFormatter" use="optional"/>
                  <xs:attribute type="xs:boolean" name="mapped" use="optional"/>
                  <xs:attribute type="xs:boolean" name="auditable" use="optional"/>
                  <xs:attribute type="xs:boolean" name="auditable" use="optional"/>
                  <xs:attribute type="xs:boolean" name="reportable" use="optional"/>
                  <xs:attribute type="xs:boolean" name="actionPolicy" use="optional"/>
                  <xs:attribute type="xs:boolean" name="changeEvents" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Tables -->
        <xs:element name="tables" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="table" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="columns" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="column" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="xs:string" name="field" use="required"/>
                                  <xs:attribute type="xs:string" name="label" use="optional"/>
                                  <xs:attribute type="xs:string" name="renderWidget" use="optional"/>
                                  <xs:attribute type="xs:string" name="linkAct" use="optional"/>
                                  <xs:attribute type="xs:integer" name="widthRatio" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="switchOnChange" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="hiddenOnNull" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="hidden" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="disabled" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="editable" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="sortable" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="summary" use="optional"/>
                                  <xs:attribute type="xs:string" name="symbol" use="optional"/>
                                  <xs:attribute type="xs:string" name="order" use="optional"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="loadings" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="loading" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="optional"/>
                              <xs:attribute type="xs:string" name="provider" use="required"/>
                              <xs:attribute type="xs:integer" name="orderIndex" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="actions" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="action" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="optional"/>
                              <xs:attribute type="xs:string" name="policy" use="required"/>
                              <xs:attribute type="xs:integer" name="orderIndex" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="quickFilters" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="filter" name="quickFilter" maxOccurs="unbounded" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="filters" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="colorfilter" name="filter" maxOccurs="unbounded" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                  <xs:attribute type="xs:string" name="description" use="required"/>
                  <xs:attribute type="xs:string" name="label" use="required"/>
                  <xs:attribute type="xs:string" name="entity" use="required"/>
                  <xs:attribute type="xs:string" name="detailsPanel" use="optional"/>
                  <xs:attribute type="xs:string" name="loadingSearchInput" use="optional"/>
                  <xs:attribute type="xs:string" name="loadingFilterGen" use="optional"/>
