<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified">
  <!-- Applet Type -->
  <xs:simpleType name="fcapplettype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="MANAGE_ENTITYLIST"/>
      <xs:enumeration value="MANAGE_ENTITYLIST_ASSIGN"/>
      <xs:enumeration value="MANAGE_ENTITYLIST_SUBMISSION"/>
      <xs:enumeration value="MANAGE_ENTITYLIST_SINGLEFORM"/>
      <xs:enumeration value="MANAGE_LOADINGLIST"/>
      <xs:enumeration value="HEADLESS_TABS"/>
      <xs:enumeration value="CREATE_ENTITY"/>
      <xs:enumeration value="CREATE_ENTITY_SINGLEFORM"/>
      <xs:enumeration value="FORM_WIZARD"/>
      <xs:enumeration value="LISTING"/>
      <xs:enumeration value="MANAGE_PROPERTYLIST"/>
      <xs:enumeration value="MY_WORKITEM"/>
      <xs:enumeration value="TASK_EXECUTION"/>
      <xs:enumeration value="DATA_IMPORT"/>
      <xs:enumeration value="FACADE"/>
      <xs:enumeration value="FACADE_MULTIPLE"/>
      <xs:enumeration value="PATH_PAGE"/>
      <xs:enumeration value="PATH_WINDOW"/>
      <xs:enumeration value="REVIEW_WORKITEMS"/>
      <xs:enumeration value="REVIEW_SINGLEFORMWORKITEMS"/>
      <xs:enumeration value="REVIEW_WIZARDWORKITEMS"/>
      <xs:enumeration value="STUDIO_FC_COMPONENT"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Data Type -->
  <xs:simpleType name="fcdatatype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CHAR"/>
      <xs:enumeration value="BOOLEAN"/>
      <xs:enumeration value="SHORT"/>
      <xs:enumeration value="INTEGER"/>
      <xs:enumeration value="LONG"/>
      <xs:enumeration value="FLOAT"/>
      <xs:enumeration value="DOUBLE"/>
      <xs:enumeration value="DECIMAL"/>
      <xs:enumeration value="DATE"/>
      <xs:enumeration value="STRING"/>
      <xs:enumeration value="TIMESTAMP"/>
      <xs:enumeration value="TIMESTAMP_UTC"/>
      <xs:enumeration value="CLOB"/>
      <xs:enumeration value="BLOB"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Input Type -->
  <xs:simpleType name="fcinputtype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CHAR"/>
      <xs:enumeration value="CHAR_ARRAY"/>
      <xs:enumeration value="BOOLEAN"/>
      <xs:enumeration value="BOOLEAN_ARRAY"/>
      <xs:enumeration value="SHORT"/>
      <xs:enumeration value="SHORT_ARRAY"/>
      <xs:enumeration value="INTEGER"/>
      <xs:enumeration value="INTEGER_ARRAY"/>
      <xs:enumeration value="LONG"/>
      <xs:enumeration value="LONG_ARRAY"/>
      <xs:enumeration value="MAPPED"/>
      <xs:enumeration value="FLOAT"/>
      <xs:enumeration value="FLOAT_ARRAY"/>
      <xs:enumeration value="DOUBLE"/>
      <xs:enumeration value="DOUBLE_ARRAY"/>
      <xs:enumeration value="DECIMAL"/>
      <xs:enumeration value="DECIMAL_ARRAY"/>
      <xs:enumeration value="DATE"/>
      <xs:enumeration value="DATE_ARRAY"/>
      <xs:enumeration value="DATETIME"/>
      <xs:enumeration value="DATETIME_ARRAY"/>
      <xs:enumeration value="STRING"/>
      <xs:enumeration value="STRING_ARRAY"/>
      <xs:enumeration value="FILE"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Entity Field Data Type -->
  <xs:simpleType name="fcfielddatatype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CHAR"/>
      <xs:enumeration value="BOOLEAN"/>
      <xs:enumeration value="BOOLEAN_ARRAY"/>
      <xs:enumeration value="SHORT"/>
      <xs:enumeration value="SHORT_ARRAY"/>
      <xs:enumeration value="INTEGER"/>
      <xs:enumeration value="INTEGER_ARRAY"/>
      <xs:enumeration value="LONG"/>
      <xs:enumeration value="LONG_ARRAY"/>
      <xs:enumeration value="TENANT_ID"/>
      <xs:enumeration value="MAPPED"/>
      <xs:enumeration value="FLOAT"/>
      <xs:enumeration value="FLOAT_ARRAY"/>
      <xs:enumeration value="DOUBLE"/>
      <xs:enumeration value="DOUBLE_ARRAY"/>
      <xs:enumeration value="DECIMAL"/>
      <xs:enumeration value="DECIMAL_ARRAY"/>
      <xs:enumeration value="DATE"/>
      <xs:enumeration value="DATE_ARRAY"/>
      <xs:enumeration value="TIMESTAMP_UTC"/>
      <xs:enumeration value="TIMESTAMP"/>
      <xs:enumeration value="TIMESTAMP_ARRAY"/>
      <xs:enumeration value="CLOB"/>
      <xs:enumeration value="BLOB"/>
      <xs:enumeration value="STRING"/>
      <xs:enumeration value="STRING_ARRAY"/>
      <xs:enumeration value="ENUM"/>
      <xs:enumeration value="ENUM_REF"/>
      <xs:enumeration value="ENUM_DYN"/>
      <xs:enumeration value="REF"/>
      <xs:enumeration value="REF_UNLINKABLE"/>
      <xs:enumeration value="REF_FILEUPLOAD"/>
      <xs:enumeration value="FOSTER_PARENT_ID"/>
      <xs:enumeration value="FOSTER_PARENT_TYPE"/>
      <xs:enumeration value="CATEGORY_COLUMN"/>
      <xs:enumeration value="SCRATCH"/>
      <xs:enumeration value="LIST_ONLY"/>
      <xs:enumeration value="CHILD"/>
      <xs:enumeration value="CHILD_LIST"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Entity Base Type -->
  <xs:simpleType name="fcentitybasetype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="BASE_ENTITY"/>
      <xs:enumeration value="BASE_VERSION_ENTITY"/>
      <xs:enumeration value="BASE_AUDIT_ENTITY"/>
      <xs:enumeration value="BASE_STATUS_ENTITY"/>
      <xs:enumeration value="BASE_WORK_ENTITY"/>
      <xs:enumeration value="BASE_STATUS_WORK_ENTITY"/>
      <xs:enumeration value="BASE_NAMED_ENTITY"/>
      <xs:enumeration value="BASE_CONFIG_ENTITY"/>
      <xs:enumeration value="BASE_CONFIG_NAMED_ENTITY"/>
      <xs:enumeration value="BASE_APPLICATION_ENTITY"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Search Condition Type -->
  <xs:simpleType name="fcsearchcondtype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="SESSION_ATTRIBUTE"/>
      <xs:enumeration value="EQUALS"/>
      <xs:enumeration value="LESS_THAN"/>
      <xs:enumeration value="LESS_OR_EQUAL"/>
      <xs:enumeration value="GREATER_THAN"/>
      <xs:enumeration value="GREATER_OR_EQUAL"/>
      <xs:enumeration value="BEGINS_WITH"/>
      <xs:enumeration value="ENDS_WITH"/>
      <xs:enumeration value="LIKE"/>
      <xs:enumeration value="ILIKE"/>
      <xs:enumeration value="NOT_EQUALS"/>
      <xs:enumeration value="NOT_LIKE"/>
      <xs:enumeration value="NOT_BEGIN_WITH"/>
      <xs:enumeration value="NOT_END_WITH"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Filter Condition Type -->
  <xs:simpleType name="fcfiltercondtype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="EQUALS"/>
      <xs:enumeration value="IEQUALS"/>
      <xs:enumeration value="EQUALS_LINGUAL"/>
      <xs:enumeration value="NOT_EQUALS"/>
      <xs:enumeration value="NOT_EQUALS_LINGUAL"/>
      <xs:enumeration value="INOT_EQUALS"/>
      <xs:enumeration value="LESS_THAN"/>
      <xs:enumeration value="LESS_THAN_LINGUAL"/>
      <xs:enumeration value="LESS_OR_EQUAL"/>
      <xs:enumeration value="LESS_OR_EQUAL_LINGUAL"/>
      <xs:enumeration value="GREATER_THAN"/>
      <xs:enumeration value="GREATER_THAN_LINGUAL"/>
      <xs:enumeration value="GREATER_OR_EQUAL"/>
      <xs:enumeration value="GREATER_OR_EQUAL_LINGUAL"/>
      <xs:enumeration value="BETWEEN"/>
      <xs:enumeration value="BETWEEN_LINGUAL"/>
      <xs:enumeration value="NOT_BETWEEN"/>
      <xs:enumeration value="NOT_BETWEEN_LINGUAL"/>
      <xs:enumeration value="AMONGST"/>
      <xs:enumeration value="NOT_AMONGST"/>
      <xs:enumeration value="LIKE"/>
      <xs:enumeration value="ILIKE"/>
      <xs:enumeration value="NOT_LIKE"/>
      <xs:enumeration value="BEGINS_WITH"/>
      <xs:enumeration value="BEGINS_WITH"/>
      <xs:enumeration value="NOT_BEGIN_WITH"/>
      <xs:enumeration value="ENDS_WITH"/>
      <xs:enumeration value="IENDS_WITH"/>
      <xs:enumeration value="NOT_END_WITH"/>
      <xs:enumeration value="IS_NULL"/>
      <xs:enumeration value="IS_NOT_NULL"/>
      <xs:enumeration value="EQUALS_FIELD"/>
      <xs:enumeration value="NOT_EQUALS_FIELD"/>
      <xs:enumeration value="EQUALS_SESSIONPARAM"/>
      <xs:enumeration value="NOT_EQUALS_SESSIONPARAM"/>
      <xs:enumeration value="LESS_THAN_FIELD"/>
      <xs:enumeration value="LESS_OR_EQUAL_FIELD"/>
      <xs:enumeration value="GREATER_THAN_FIELD"/>
      <xs:enumeration value="GREATER_OR_EQUAL_FIELD"/>
      <xs:enumeration value="BETWEEN_FIELD"/>
      <xs:enumeration value="NOT_BETWEEN_FIELD"/>
      <xs:enumeration value="LIKE_FIELD"/>
      <xs:enumeration value="ILIKE_FIELD"/>
      <xs:enumeration value="NOT_LIKE_FIELD"/>
      <xs:enumeration value="BEGINS_WITH_FIELD"/>
      <xs:enumeration value="IBEGINS_WITH_FIELD"/>
      <xs:enumeration value="NOT_BEGIN_WITH_FIELD"/>
      <xs:enumeration value="ENDS_WITH_FIELD"/>
      <xs:enumeration value="IENDS_WITH_FIELD"/>
      <xs:enumeration value="NOT_END_WITH_FIELD"/>
      <xs:enumeration value="EQUALS_PARAM"/>
      <xs:enumeration value="NOT_EQUALS_PARAM"/>
      <xs:enumeration value="LESS_THAN_PARAM"/>
      <xs:enumeration value="LESS_OR_EQUAL_PARAM"/>
      <xs:enumeration value="GREATER_THAN_PARAM"/>
      <xs:enumeration value="GREATER_OR_EQUAL_PARAM"/>
      <xs:enumeration value="BETWEEN_PARAM"/>
      <xs:enumeration value="NOT_BETWEEN_PARAM"/>
      <xs:enumeration value="LIKE_PARAM"/>
      <xs:enumeration value="ILIKE_PARAM"/>
      <xs:enumeration value="NOT_LIKE_PARAM"/>
      <xs:enumeration value="BEGINS_WITH_PARAM"/>
      <xs:enumeration value="IBEGINS_WITH_PARAM"/>
      <xs:enumeration value="NOT_BEGIN_WITH_PARAM"/>
      <xs:enumeration value="ENDS_WITH_PARAM"/>
      <xs:enumeration value="IENDS_WITH_PARAM"/>
      <xs:enumeration value="NOT_END_WITH_PARAM"/>
      <xs:enumeration value="EQUALS_COLLECTION"/>
      <xs:enumeration value="NOT_EQUALS_COLLECTION"/>
      <xs:enumeration value="LESS_THAN_COLLECTION"/>
      <xs:enumeration value="LESS_OR_EQUAL_COLLECTION"/>
      <xs:enumeration value="GREATER_THAN_COLLECTION"/>
      <xs:enumeration value="GREATER_OR_EQUAL_COLLECTION"/>
      <xs:enumeration value="AND"/>
      <xs:enumeration value="OR"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Tristate Type -->
  <xs:simpleType name="fctristate">
    <xs:restriction base="xs:string">
      <xs:enumeration value="TRUE"/>
      <xs:enumeration value="FALSE"/>
      <xs:enumeration value="CONFORMING"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Field Sequence -->
  <xs:complexType name="fcfieldsequence">
    <xs:sequence>
      <xs:element name="entry" maxOccurs="unbounded" minOccurs="0">
        <xs:complexType>
          <xs:attribute type="xs:string" name="field" use="required"/>
          <xs:attribute type="xs:string" name="formatter" use="optional"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- Property Sequence-->
  <xs:complexType name="fcpropertysequence">
    <xs:sequence>
      <xs:element name="entry" maxOccurs="unbounded" minOccurs="0">
        <xs:complexType>
          <xs:attribute type="xs:string" name="property" use="required"/>
          <xs:attribute type="xs:string" name="label" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- Restriction -->
  <xs:complexType name="fcrestriction">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="restriction" type="fcrestriction"/>
    </xs:choice>
    <xs:attribute type="fcfiltercondtype" name="type" use="required"/>
    <xs:attribute type="xs:string" name="field" use="optional"/>
    <xs:attribute type="xs:string" name="paramA" use="optional"/>
    <xs:attribute type="xs:string" name="paramB" use="optional"/>
  </xs:complexType>
  <!-- Filter -->
  <xs:complexType name="fcfilter">
    <xs:sequence>
      <xs:element type="fcrestriction" name="restriction" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="optional"/>
    <xs:attribute type="xs:string" name="description" use="optional"/>
    <xs:attribute type="xs:string" name="label" use="optional"/>
    <xs:attribute type="xs:string" name="filterGenerator" use="optional"/>
    <xs:attribute type="xs:string" name="filterGeneratorRule" use="optional"/>
    <xs:attribute type="xs:boolean" name="quickFilter" use="optional"/>
  </xs:complexType>
  <!-- Filter with Color -->
  <xs:complexType name="fccolorfilter">
    <xs:complexContent>
      <xs:extension base="fcfilter">
        <xs:attribute type="xs:string" name="rowColor" use="optional"/>
        <xs:attribute type="xs:string" name="legendLabel" use="optional"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!-- Set Values -->
  <xs:complexType name="fcsetvalues">
    <xs:sequence>
      <xs:element name="setValue" maxOccurs="unbounded" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="type" use="optional">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="NULL"/>
                <xs:enumeration value="IMMEDIATE"/>
                <xs:enumeration value="IMMEDIATE_LINGUAL"/>
                <xs:enumeration value="IMMEDIATE_FIELD"/>
                <xs:enumeration value="PROCESS_VARIABLE"/>
                <xs:enumeration value="SESSION_PARAMETER"/>
                <xs:enumeration value="GENERATOR"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute type="xs:string" name="field" use="required"/>
          <xs:attribute type="xs:string" name="value" use="optional"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="optional"/>
    <xs:attribute type="xs:string" name="description" use="optional"/>
    <xs:attribute type="xs:string" name="label" use="optional"/>
  </xs:complexType>
  <!-- Application -->
  <xs:element name="application">
    <xs:complexType>
      <xs:all minOccurs="0" maxOccurs="1">
        <!-- Reports -->
        <xs:element name="reports" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="report" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="xs:string" name="configFile" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Notification Templates -->
        <xs:element name="notifTemplates" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="notifTemplate" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="xs:string" name="configFile" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Workflows -->
        <xs:element name="workflows" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="workflow" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="xs:string" name="configFile" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Applets -->
        <xs:element name="applets" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="applet" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType mixed="true">
                  <xs:all>
                    <xs:element name="properties" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="property" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="value" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="routeToAppletItems" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="routeToAppletItem" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="routeToApplet" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="alerts" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="alert" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="optional"/>
                              <xs:attribute type="xs:string" name="sender" use="optional"/>
                              <xs:attribute type="xs:string" name="template" use="optional"/>
                              <xs:attribute type="xs:string" name="recipientPolicy" use="optional"/>
                              <xs:attribute type="xs:string" name="recipientNameRule" use="optional"/>
                              <xs:attribute type="xs:string" name="recipientContactRule" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="filters" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="filter" type="fcfilter" maxOccurs="unbounded" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="valuesset" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="values" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="setValues" type="fcsetvalues" maxOccurs="1" minOccurs="0"/>
                              </xs:sequence>
                              <xs:attribute type="xs:string" name="name" use="required"/>
                              <xs:attribute type="xs:string" name="description" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="optional"/>
                              <xs:attribute type="xs:string" name="valueGenerator" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                  <xs:attribute type="fcapplettype" name="type" use="required"/>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                  <xs:attribute type="xs:string" name="description" use="required"/>
                  <xs:attribute type="xs:string" name="label" use="required"/>
                  <xs:attribute type="xs:string" name="entity" use="optional"/>
                  <xs:attribute type="xs:string" name="icon" use="optional"/>
                  <xs:attribute type="xs:string" name="path" use="optional"/>
                  <xs:attribute type="xs:string" name="titleFormat" use="optional"/>
                  <xs:attribute type="xs:integer" name="displayIndex" use="optional"/>
                  <xs:attribute type="xs:boolean" name="menuAccess" use="optional"/>
                  <xs:attribute type="xs:boolean" name="supportOpenInNewWindow" use="optional"/>
                  <xs:attribute type="xs:boolean" name="allowSecondaryTenants" use="optional"/>
                  <xs:attribute type="xs:string" name="routeToApplet" use="optional"/>
                  <xs:attribute type="xs:string" name="baseField" use="optional"/>
                  <xs:attribute type="xs:string" name="assignField" use="optional"/>
                  <xs:attribute type="xs:string" name="assignDescField" use="optional"/>
                  <xs:attribute type="xs:string" name="pseudoDeleteField" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Widget Types -->
        <xs:element name="widgetTypes" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="widgetType" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:attribute type="fcdatatype" name="dataType" use="required"/>
                  <xs:attribute type="fcinputtype" name="inputType" use="optional"/>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                  <xs:attribute type="xs:string" name="description" use="required"/>
                  <xs:attribute type="xs:string" name="editor" use="required"/>
                  <xs:attribute type="xs:string" name="renderer" use="optional"/>
                  <xs:attribute type="xs:boolean" name="stretch" use="optional"/>
                  <xs:attribute type="xs:boolean" name="listOption" use="optional"/>
                  <xs:attribute type="xs:boolean" name="enumOption" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- References -->
        <xs:element name="references" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="reference" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="filters" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="filter" type="fcfilter" maxOccurs="1" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                  <xs:attribute type="xs:string" name="name" use="required"/>
                  <xs:attribute type="xs:string" name="description" use="required"/>
                  <xs:attribute type="xs:string" name="entity" use="required"/>
                  <xs:attribute type="xs:string" name="searchField" use="optional"/>
                  <xs:attribute type="xs:string" name="orderField" use="optional"/>
                  <xs:attribute type="xs:string" name="searchTable" use="optional"/>
                  <xs:attribute type="xs:string" name="selectHandler" use="optional"/>
                  <xs:attribute type="xs:string" name="listFormat" use="optional"/>
                  <xs:attribute type="xs:string" name="filterGenerator" use="optional"/>
                  <xs:attribute type="xs:string" name="filterGeneratorRule" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Entities -->
        <xs:element name="entities" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="entity" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType mixed="true">
                  <xs:all>
                    <xs:element name="fields" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="field" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="fcfielddatatype" name="type" use="required"/>
                                  <xs:attribute name="case" use="optional">
                                    <xs:simpleType>
                                      <xs:restriction base="xs:string">
                                        <xs:enumeration value="LOWER"/>
                                        <xs:enumeration value="UPPER"/>
                                        <xs:enumeration value="CAMEL"/>
                                        <xs:enumeration value="CAPITAL"/>
                                        <xs:enumeration value="PATH"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:attribute>
                                  <xs:attribute type="xs:string" name="name" use="required"/>
                                  <xs:attribute type="xs:string" name="label" use="required"/>
                                  <xs:attribute type="xs:string" name="inputLabel" use="optional"/>
                                  <xs:attribute type="xs:string" name="inputWidget" use="optional"/>
                                  <xs:attribute type="xs:string" name="category" use="optional"/>
                                  <xs:attribute type="xs:integer" name="maxLen" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="trim" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="allowNegative" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="readOnly" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="nullable" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="auditable" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="reportable" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="descriptive" use="optional"/>
                                  <xs:attribute type="xs:boolean" name="basicSearch" use="optional"/>
