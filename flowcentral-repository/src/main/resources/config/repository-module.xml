<?xml version="1.0"?>
<!--Repository module configuration -->
<module
	xmlns="http://flowcentraltech.com/schema-common"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://flowcentraltech.com/schema-common file:///C:/data/flowcentral-module-4.0.0.xsd"
	name="repository" description="$m{repository.module}"
	shortCode="RPO" label="$m{repository.label}">
	<!-- Applications -->
	<applications>
		<application name="repository"
			shortDescription="$m{repository.short.description}"
			longDescription="$m{repository.long.description}" autoInstall="true"
			configFile="apps/repository-application.xml" />
	</applications>

	<!-- System Parameters -->
</module>